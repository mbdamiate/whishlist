    "test:unit:watch": "mocha ./src/api/**/*.test.js --watch",
    "test:integration": "mocha ./tests/**/*.test.js",
    "gen:jsdoc": "jsdoc --configure ./jsdoc.json --verbose",
    "gen:apidoc": "apidoc -i src -o ./apidocs"
