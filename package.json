  "scripts": {
    "docker:dev:up": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up --build",
    "docker:dev:down": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml down --rmi local --volumes --remove-orphans",
